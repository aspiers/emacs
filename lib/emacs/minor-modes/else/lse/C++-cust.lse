    /NOAUTO_SUBSTITUTE -
    /DESCRIPTION="public, protected, private"
    /DUPLICATION=VERTICAL -
    /SEPARATOR="" -
    /TYPE=MENU

    "public"
    "protected"
    "private"

END DEFINE


# Stephe's customized C++ templates

DELETE PLACEHOLDER ",..." -
    /LANGUAGE="C++" -
DEFINE PLACEHOLDER ",..." -
    /LANGUAGE="C++" -
    /NOAUTO_SUBSTITUTE -
    /DESCRIPTION=""
    /DUPLICATION=CONTEXT_DEPENDENT -
    /SEPARATOR="" -
    /TYPE=NONTERMINAL -
    
    ", ..."

END DEFINE

DELETE PLACEHOLDER "*&" -
    /LANGUAGE="C++" -
DEFINE PLACEHOLDER "*&" -
    /LANGUAGE="C++" -
    /NOAUTO_SUBSTITUTE -
    /DESCRIPTION=""
    /DUPLICATION=CONTEXT_DEPENDENT -
    /SEPARATOR="" -
    /TYPE=MENU -
    
    " *"
    " * const "
    " &"

END DEFINE

